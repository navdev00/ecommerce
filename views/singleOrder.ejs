<%-include('partials/header');-%>

    <section class="status" id="header">

        <div class="container mx-auto">
            <div class=" status-box w-75 mx-auto">
                <div class="d-flex items-center justify-between mb-12 mt-5">
                    <h5>Track Delivery Status</h5>
                    <p class="ml-auto ">
                        <%=order._id%>
                    </p>
                    <input id="hiddenInput" type="hidden" value="<%= JSON.stringify(order)%>">
                </div>


                <div class="card mb-3 mt-3">
                    <div class="p-4 text-center text-white text-lg bg-dark rounded-top"><span class="text-uppercase"> Order No - <%=order._id%></span><span class="text-medium"></span></div>
                    <!-- <div class="d-flex flex-wrap flex-sm-nowrap justify-content-between py-3 px-2 bg-secondary"> -->
                      <!-- <div class="w-100 text-center py-1 px-2"><span class="text-medium">Shipped Via:</span> UPS Ground</div>
                     
                     <div class="w-100 text-center py-1 px-2"><span class="text-medium">Expected Date:</span> SEP 09, 2017</div>  -->
                    </div> 

                   <!-- <div class="w-100 text-center py-1 px-2 mt-3"><span class="text-medium">Status:</span> <%=order.status%></div> -->
                    
                    <div class="card-body">
                        <div
                            class="steps d-flex flex-wrap flex-sm-nowrap justify-content-between padding-top-2x padding-bottom-1x">
                            
                            <div class="order-status step" data-status="order_placed">
                                <div class="step-icon-wrap">
                                    <div class="step-icon"><i class="fas fa-shopping-cart"></i></div>
                                </div>
                                <h4 class="step-title mb-3">Order Placed</h4>
                                
                            </div>

                            <div class="order-status step" data-status="confirmed">
                                <div class="step-icon-wrap">
                                    <div class="step-icon"><i class="fas fa-check-square"></i></div>
                                </div>
                                <h4 class="step-title mb-3">Processing Order</h4>
                                
                                
                            </div>

                            <div class="order-status step" data-status="confirmed">
                                <div class="step-icon-wrap">
                                    <div class="step-icon"><i class="fas fa-check-square"></i></div>
                                </div>
                                <h4 class="step-title mb-3">Quality check</h4>
                                
                                
                            </div>

                            


                            <div class="order-status step" data-status="prepared">
                                <div class="step-icon-wrap">
                                    <div class="step-icon"><i class="fas fa-plane"></i></div>
                                </div>
                                <h4 class="step-title mb-3">Product Dispatched</h4>
                            </div>

                            <div class="order-status step" data-status="delivered">
                                <div class="step-icon-wrap">
                                    <div class="step-icon"><i class="fas fa-home"></i></div>
                                </div>
                                <h4 class="step-title mb-3">Out for Delivery</h4>
                            </div>

                            
                            <div class="order-status step" data-status="Completed">
                                <div class="step-icon-wrap">
                                    <div class="step-icon"><i class="fas fa-handshake"></i></div>
                                </div>
                                <h4 class="step-title mb-3">Completed</h4>
                            </div>

                        </div>
                    </div>
                </div>
                <!-- <div class="d-flex flex-wrap flex-md-nowrap justify-content-center justify-content-sm-between align-items-center">
                    <div class="custom-control custom-checkbox mr-3">
                      <input class="custom-control-input" type="checkbox" id="notify_me" checked="">
                      <label class="custom-control-label" for="notify_me">Notify me when order is delivered</label>
                    </div>
                    <div class="text-left text-sm-right"><a class="btn btn-outline-primary btn-rounded btn-sm" href="orderDetails" data-toggle="modal" data-target="#orderDetails">View Order Details</a></div>
                  </div> -->


            </div>


        </div>


    </section>






    <%- include('partials/footer'); -%>